# Database Configuration
DATABASE_URL=mongodb://localhost:27017/travio
POSTGRES_URL=postgresql://username:password@localhost:5432/travio

# API Keys
GOOGLE_MAPS_API_KEY=your_google_maps_api_key
GOOGLE_PLACES_API_KEY=your_google_places_api_key

# Authentication
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=7d

# OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
FACEBOOK_APP_ID=your_facebook_app_id
FACEBOOK_APP_SECRET=your_facebook_app_secret

# AI/ML Services
OPENAI_API_KEY=your_openai_api_key
TENSORFLOW_SERVING_URL=http://localhost:8501

# Emergency Services
EMERGENCY_API_KEY=your_emergency_services_api_key
SMS_SERVICE_API_KEY=your_sms_service_api_key

# File Storage
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_S3_BUCKET=travio-uploads
AWS_REGION=us-east-1

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Email Service
SENDGRID_API_KEY=your_sendgrid_api_key
FROM_EMAIL=noreply@travio.com

# Push Notifications
FCM_SERVER_KEY=your_fcm_server_key
APNS_KEY_ID=your_apns_key_id
APNS_TEAM_ID=your_apns_team_id

# Payment Gateway
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key

# Environment
NODE_ENV=development
PORT=3000
API_BASE_URL=http://localhost:3000/api

# Booking APIs
BOOKING_COM_API_KEY=your_booking_com_api_key
AMADEUS_API_KEY=your_amadeus_api_key
AMADEUS_API_SECRET=your_amadeus_api_secret

# Weather API
OPENWEATHER_API_KEY=your_openweather_api_key

# Analytics
GOOGLE_ANALYTICS_ID=your_google_analytics_id
MIXPANEL_TOKEN=your_mixpanel_token

# Security
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
LOG_FILE_PATH=./logs/app.log